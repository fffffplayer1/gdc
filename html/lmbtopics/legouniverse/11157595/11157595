;(function($) {

$("textarea").keyup(function () {
    var left = 2000 - $(this).val().replace(/(\r\n|\n|\r)/g,"").length;
    if (left < 0) {
        left = 0;
    }

if (!$(this).siblings('div#quickreply-charcount').length){
$(this).after('<div id="quickreply-charcount"></div>');
}
$(this).siblings('div#quickreply-charcount').html((left == 0 ? "<span style='font-weight:bold; color: #f00;'>" : "<span>") + left+" characters remaining</span>");

});
})(LITHIUM.jQuery);

