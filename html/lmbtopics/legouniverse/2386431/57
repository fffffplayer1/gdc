	<div class="">
		<div class="lia-js-resize-images lia-component-forums-widget-board-message-view"><div data-message-id="2387283">
	<div class="lia-panel-intermission"><!-- container for custom hooks --></div>
	<a name="M367322"> </a>
	
	<a name="U2387283"> </a>
	
	

	

	
	
	
		<div class="lia-inline-ajax-feedback">
			<div class="AjaxFeedback" id="ajaxfeedback_13"></div>
			
	

	

